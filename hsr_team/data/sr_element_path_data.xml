<odoo>
    <data noupdate="1">
        <!-- CHARACTER ELEMENTS -->
        <record model="sr.element" id="sr_element_wind">
            <field name="name">Wind</field>
            <field name="reference">Wind</field>
        </record>
        <record model="sr.element" id="sr_element_ice">
            <field name="name">Ice</field>
            <field name="reference">Ice</field>
        </record>
        <record model="sr.element" id="sr_element_physical">
            <field name="name">Physical</field>
            <field name="reference">Physical</field>
        </record>
        <record model="sr.element" id="sr_element_fire">
            <field name="name">Fire</field>
            <field name="reference">Fire</field>
            <field name="img_id" eval="hsr_warp.icon_fire"/>
        </record>
        <record model="sr.element" id="sr_element_quantum">
            <field name="name">Quantum</field>
            <field name="reference">Quantum</field>
        </record>
        <record model="sr.element" id="sr_element_lightning">
            <field name="name">Lightning</field>
            <field name="reference">Lightning</field>
        </record>
        <record model="sr.element" id="sr_element_imaginary">
            <field name="name">Imaginary</field>
            <field name="reference">Imaginary</field>
        </record>

        <!-- CHARACTER PATH -->
        <record model="sr.path" id="sr_path_destruction">
            <field name="name">Destruction</field>
            <field name="reference">Warrior</field>
        </record>
        <record model="sr.path" id="sr_path_hunt">
            <field name="name">Hunt</field>
            <field name="reference">Rogue</field>
        </record>
        <record model="sr.path" id="sr_path_abundance">
            <field name="name">Abundance</field>
            <field name="reference">Priest</field>
        </record>
        <record model="sr.path" id="sr_path_erudition">
            <field name="name">Erudition</field>
            <field name="reference">Mage</field>
        </record>
        <record model="sr.path" id="sr_path_harmony">
            <field name="name">Harmony</field>
            <field name="reference">Shaman</field>
        </record>
        <record model="sr.path" id="sr_path_nihility">
            <field name="name">Nihility</field>
            <field name="reference">Warlock</field>
        </record>
        <record model="sr.path" id="sr_path_preservation">
            <field name="name">Preservation</field>
            <field name="reference">Knight</field>
        </record>
    </data>
</odoo>